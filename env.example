# Core config
NYRAG_CONFIG=/app/configs/example.yml
NYRAG_LOCAL=1
NYRAG_VESPA_DEPLOY=1
NYRAG_VESPA_ALLOW_CONTENT_CLUSTER_REMOVAL=0

# Vespa (cloud)
VESPA_CLOUD_TENANT=your-tenant
VESPA_CLOUD_APPLICATION=your-application
VESPA_CLOUD_INSTANCE=default
VESPA_CLOUD_API_KEY_PATH=/path/to/vespa-api-key.pem
VESPA_CLOUD_API_KEY=your-api-key
VESPA_CLIENT_CERT=/path/to/data-plane-public-cert.pem
VESPA_CLIENT_KEY=/path/to/data-plane-private-key.pem
VESPA_CA_CERT=/path/to/ca.pem
VESPA_TLS_VERIFY=true

# Embeddings
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# LLM (OpenAI-compatible)
LLM_BASE_URL=https://openrouter.ai/api/v1
LLM_MODEL=openai/gpt-5.1
LLM_API_KEY=your-llm-api-key
